Algoritmo "crecentes"

Var
   x, y : inteiro

Inicio
   escreval("Digite dois numero: ")
   leia(x)
   leia(y)
   
   enquanto x <> y faca

      se x < y entao
       escreval("CRESCENTE")
    senao
       escreval("DECRESCENTE")
    fimse

       escreval("Dogite outros dois numeros: ")
       leia(x)
       leia(y)
       
       fimenquanto
      

Fimalgoritmo